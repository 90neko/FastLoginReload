<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<!--

  This mapping demonstrates content-based discrimination for the
  table-per-hierarchy mapping strategy, using a formula
  discriminator.

-->

<hibernate-mapping package="com.ksptooi.entity.dto.player">

    <class name="PlayerLocation" table="PlayerLocation" lazy="true" >

        <id name="id">
            <generator class="native"/>
        </id>

        <property name="pid" column="pid"/>
        <property name="world" column="world"/>
        <property name="x" column="x"/>
        <property name="y" column="y"/>
        <property name="z" column="z"/>
        <property name="pitch" column="pitch"/>
        <property name="yaw" column="yaw"/>

    </class>



</hibernate-mapping>